# Doxyfile 1.1.5

#---------------------------------------------------------------------------
# General configuration options
#---------------------------------------------------------------------------
PROJECT_NAME          = @PACKAGE@
PROJECT_NUMBER        = @VERSION@
OUTPUT_DIRECTORY      = sourcedoc
OUTPUT_LANGUAGE       = English
DISABLE_INDEX         = NO
EXTRACT_ALL           = YES
EXTRACT_PRIVATE       = YES
EXTRACT_STATIC        = YES
HIDE_UNDOC_MEMBERS    = NO
HIDE_UNDOC_CLASSES    = NO
BRIEF_MEMBER_DESC     = YES
REPEAT_BRIEF          = YES
ALWAYS_DETAILED_SEC   = NO
FULL_PATH_NAMES       = YES
STRIP_FROM_PATH       = @TOP_SRCDIR@
INTERNAL_DOCS         = YES
CLASS_DIAGRAMS        = YES
SOURCE_BROWSER        = YES
INLINE_SOURCES        = NO
STRIP_CODE_COMMENTS   = YES
CASE_SENSE_NAMES      = YES
HIDE_SCOPE_NAMES      = NO
VERBATIM_HEADERS      = YES
SHOW_INCLUDE_FILES    = YES
JAVADOC_AUTOBRIEF     = YES
INHERIT_DOCS          = YES
INLINE_INFO           = YES
SORT_MEMBER_DOCS      = NO
TAB_SIZE              = 8
ENABLED_SECTIONS      =
#---------------------------------------------------------------------------
# configuration options related to warning and progress messages
#---------------------------------------------------------------------------
QUIET                 = NO
WARNINGS              = YES
WARN_IF_UNDOCUMENTED  = YES
WARN_FORMAT           = "$file:$line: $text"
#---------------------------------------------------------------------------
# configuration options related to the input files
#---------------------------------------------------------------------------
INPUT                 = @TOP_SRCDIR@ ../config.h
FILE_PATTERNS         = *.cpp *.cc *.c *.h
RECURSIVE             = YES
EXCLUDE               = @TOP_SRCDIR@/docs
EXCLUDE_PATTERNS      = acconfig.h
EXAMPLE_PATH          =
EXAMPLE_PATTERNS      =
IMAGE_PATH            =
INPUT_FILTER          =
#---------------------------------------------------------------------------
# configuration options related to the alphabetical class index
#---------------------------------------------------------------------------
ALPHABETICAL_INDEX    = YES
COLS_IN_ALPHA_INDEX   = 3
IGNORE_PREFIX         =
#---------------------------------------------------------------------------
# configuration options related to the HTML output
#---------------------------------------------------------------------------
GENERATE_HTML         = YES
HTML_OUTPUT           = html
HTML_HEADER           = @TOP_SRCDIR@/docs/doxygen_full_header.html_tmpl
HTML_FOOTER           = @TOP_SRCDIR@/docs/doxygen_full_footer.html_tmpl
HTML_STYLESHEET       =
HTML_ALIGN_MEMBERS    = YES
GENERATE_HTMLHELP     = NO
#---------------------------------------------------------------------------
# configuration options related to the LaTeX output
#---------------------------------------------------------------------------
GENERATE_LATEX        = YES
LATEX_OUTPUT          = latex
COMPACT_LATEX         = NO
PAPER_TYPE            = a4
EXTRA_PACKAGES        =
LATEX_HEADER          =
PDF_HYPERLINKS        = YES
LATEX_BATCHMODE       = YES
#---------------------------------------------------------------------------
# configuration options related to the RTF output
#---------------------------------------------------------------------------
GENERATE_RTF          = NO
RTF_OUTPUT            = rtf
COMPACT_RTF           = NO
RTF_HYPERLINKS        = NO
RTF_STYLESHEET_FILE   =
#---------------------------------------------------------------------------
# configuration options related to the man page output
#---------------------------------------------------------------------------
GENERATE_MAN          = YES
MAN_OUTPUT            = man
MAN_EXTENSION         = .3
#---------------------------------------------------------------------------
# Configuration options related to the preprocessor
#---------------------------------------------------------------------------
ENABLE_PREPROCESSING  = YES
MACRO_EXPANSION       = YES
EXPAND_ONLY_PREDEF    = YES
SEARCH_INCLUDES       = YES
INCLUDE_PATH          =
PREDEFINED            =
EXPAND_AS_DEFINED     = DEFINE_ERROR_BASECLASS DEFINE_ERROR_CLASS
#---------------------------------------------------------------------------
# Configuration::addtions related to external references
#---------------------------------------------------------------------------
TAGFILES              =
GENERATE_TAGFILE      =
ALLEXTERNALS          = NO
PERL_PATH             = @PERL_PATH@
#---------------------------------------------------------------------------
# Configuration options related to the dot tool
#---------------------------------------------------------------------------
HAVE_DOT              = @DOXYGEN_HAVE_DOT@
CLASS_GRAPH           = YES
COLLABORATION_GRAPH   = YES
INCLUDE_GRAPH         = YES
INCLUDED_BY_GRAPH     = YES
GRAPHICAL_HIERARCHY   = YES
DOT_PATH              = @DOXYGEN_DOT_PATH@
MAX_DOT_GRAPH_WIDTH   = 800
MAX_DOT_GRAPH_HEIGHT  = 1024
#---------------------------------------------------------------------------
# Configuration::addtions related to the search engine
#---------------------------------------------------------------------------
SEARCHENGINE          = NO
CGI_NAME              = search.cgi
CGI_URL               =
DOC_URL               =
DOC_ABSPATH           =
BIN_ABSPATH           = /usr/local/bin/
EXT_DOC_PATHS         =
