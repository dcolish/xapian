$set{thousand,$.}$set{decimal,.}$setmap{BN,,Any Country,uk,England,fr,France}
<html>
$set{topterms,$or{$ne{$msize,0},$query}}
$if{topterms,$include{toptermsjs}}
<head>
<title>$if{$query,Omega search results for `$html{$query}',New Omega Search}</title>
</head>
<body bgcolor='#eeeeee'>
<FORM NAME=P METHOD=GET 
ACTION="$html{$or{$env{SCRIPT_NAME},omega}}" TARGET="_top">
<center>
<INPUT NAME=P VALUE="$html{$query}" SIZE=65>
<INPUT TYPE=SUBMIT VALUE="Search" BORDER=0>
<hr>
$if{,$include{anyalldropbox},$include{anyallradio}}
$if{$opt{topterms},
 <hr>
 <table><tr><td bgcolor="#ccffcc">
 $map{$topterms,<INPUT TYPE=checkbox NAME=X VALUE="$_." onClick="C(this)">&nbsp;$_. }
 $if{$topterms,<BR><NOSCRIPT><INPUT TYPE=hidden NAME=ADD VALUE=1></NOSCRIPT>}
 </td></tr></table>
 <hr>
}
$if{$eq{$msize,0},

$if{$query,No documents match your query},

$if{$ne{$last,$msize},
 $add{$topdoc,1}-$add{$topdoc,$hitsperpage} of about $msize matches,
 $if{$eq{$topdoc,0},
  All $msize matches,
  $add{$topdoc,1}$if{$ne{$add{$topdoc,1},$msize},-$msize} of exactly $msize matches}
}
<hr>
</center>
$list{$freqs,Term frequencies: <b>,$. ,</b>}
<table>
$hitlist{<TR><TD VALIGN=top><IMG
SRC="images/score-$score.png"
ALT="$percentage%" HEIGHT=16 WIDTH=32></TD>
<TD VALIGN=top><TABLE BORDER=0 CELLPADDING=1><TR><TD BGCOLOR="#ccffcc">
<INPUT TYPE=checkbox NAME=R VALUE=$id$if{$relevant, CHECKED}>
</TD></TR></TABLE></TD>
<TD><B><A
HREF="$field{url}">$html{$or{$field{caption},$field{url},Untitled}}</A></B><BR>
$highlight{$field{sample},$terms}$if{$field{sample},...}<br>
<A HREF="$field{url}">$html{$field{url}}</A><br>
${
<small>Language: <b>$html{$or{$field{language},unknown}}</b>
Size: <b>$html{$or{$filesize{$field{size}},unknown}}</b>
Last modified: <b>$html{$or{$date{$field{modified}},unknown}}</b><br>
}
$percentage% relevant, matching:
<i>$list{$html{$terms},$. ,</i> and <i>}</i></small>${for lynx:}<P></TD></TR>}
</table>
${$map{$range{1,$lastpage},<INPUT TYPE=submit NAME="[" VALUE="$_">}}

<br><center>
$if{$ne{$topdoc,0},<INPUT TYPE=image NAME="&lt;" ALT="&lt;"
SRC="images/previous.png" BORDER=0 HEIGHT=30
WIDTH=30>,
<IMG ALT="" SRC="images/prevoff.png" HEIGHT=30 WIDTH=30>}

${suppress page links if there's only one page}
$if{$ne{$lastpage,1},
$set{a,$if{$opt{pagelink_height}, HEIGHT=$opt{pagelink_height}}$if{$opt{pagelink_width}, WIDTH=$opt{pagelink_width}}}

$map{$range{1,$add{$thispage,-1}},
<INPUT TYPE=image NAME="[ $_ ]" VALUE="$_"
SRC="images/page-$_.png" BORDER=0$opt{a} ALT="$_">}

<IMG SRC="images/page-$thispage{}s.png"$opt{a}
ALT=$thispage>

$map{$range{$add{$thispage,1},$min{$lastpage,$add{$thispage,9}}},
<INPUT TYPE=image NAME="[ $_ ]" VALUE="$_"
SRC="images/page-$_.png" BORDER=0$opt{a} ALT="$_">}
}

$if{$ne{$last,$msize},<INPUT TYPE=image NAME="&gt;" ALT="&gt;"
SRC="images/next.png" BORDER=0 HEIGHT=30 WIDTH=30>,
<IMG ALT="" SRC="images/nextoff.png" HEIGHT=30 WIDTH=30>}
</center><br>

}
$if{$dbname,<INPUT TYPE=hidden NAME=DB VALUE="$html{$dbname}">}
$if{$ne{$topdoc,0},<INPUT TYPE=hidden NAME=TOPDOC VALUE=$topdoc>}
$if{$ne{$hitsperpage,10},<INPUT TYPE=hidden NAME=HITSPERPAGE VALUE=$hitsperpage>}
$if{$fmt,<INPUT TYPE=hidden NAME=FMT VALUE="$html{$fmt}">}
$list{$queryterms,<INPUT TYPE=hidden NAME=OLDP VALUE=",.,.">}
$list{$relevants,<INPUT TYPE=hidden NAME=R VALUE=",.,">}
$if{$cgi{THRESHOLD},<INPUT TYPE=hidden NAME=THRESHOLD VALUE="$html{$cgi{THRESHOLD}}">}
</FORM>
